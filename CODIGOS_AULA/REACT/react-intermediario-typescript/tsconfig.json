  "compilerOptions": {
    "target": "es2016",   
    "jsx": "react-jsx",                              
    "esModuleInterop": true,                                               
    "forceConsistentCasingInFileNames": true,            
    "strict": true,                                      
    "skipLibCheck": true,
    "moduleResolution":"node",                              
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
